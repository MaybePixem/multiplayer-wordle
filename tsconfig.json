{
	"compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"noImplicitAny": false,
		"removeComments": true,
		"experimentalDecorators": true,
		"rootDir": ".",
		"sourceMap": true,
		"listFiles": true,
		"noEmitOnError": true,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
		"allowJs": false,
		"outDir": "build/dist",
		"noUnusedLocals": true,
		"strict": true,
		"jsx": "react-jsx",
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"src"
	]
}
